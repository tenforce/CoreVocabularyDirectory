DB.DBA.VHOST_REMOVE (
	 lhost=>'*ini*',
	 vhost=>'*ini*',
	 lpath=>'/vdm'
);

DB.DBA.VHOST_DEFINE (
	 lhost=>'*ini*',
	 vhost=>'*ini*',
	 lpath=>'/vdm',
	 ppath=>'/vdm/www/',
	 is_dav=>0,
	 def_page=>'index.html',
	 vsp_user=>'dba',
	 ses_vars=>1,
	 opts=>vector ('xml_templates', 'yes', 'executable', 'yes', 'browse_sheet', '', 'url_rewrite', 'http_rule_list_5'),
	 is_default_host=>0
);

DB.DBA.URLREWRITE_CREATE_RULELIST ( 
'http_rule_list_5', 1, 
  vector ('http_rule_9', 'http_rule_10', 'http_rule_11', 'http_rule_12'));

DB.DBA.URLREWRITE_CREATE_REGEX_RULE ( 
'http_rule_9', 1, 
  '/vdm/id/(.*)', 
vector ('par_1'), 
1, 
'/vdm/doc/%s', 
vector ('par_1'), 
NULL, 
NULL, 
1, 
303, 
'' 
);

DB.DBA.URLREWRITE_CREATE_REGEX_RULE ( 
'http_rule_10', 1, 
  '/vdm/doc/([^/.]*)(?:/([^/.]*))?(?:\.(.*))?', 
vector ('par_1', 'par_2', 'par_3', 'par_4'), 
4, 
'/vdm/description.vsp?namespace=%U&type=%U&id=%U&format=%U', 
vector ('par_1', 'par_2', 'par_3', 'par_4'), 
NULL, 
NULL, 
1, 
303, 
'' 
);

DB.DBA.URLREWRITE_CREATE_REGEX_RULE ( 
'http_rule_11', 1, 
  '/vdm/about/([^/]*)/(.*)', 
vector ('par_1', 'par_2'), 
2, 
'/vdm/description.vsp?format=%U&uri=%s', 
vector ('par_1', 'par_2'), 
NULL, 
NULL, 
1, 
0, 
'' 
);

DB.DBA.URLREWRITE_CREATE_REGEX_RULE ( 
'http_rule_12', 1, 
  '/vdm/search(.*)', 
vector ('par_1'), 
1, 
'/vdm/search.vsp%s', 
vector ('par_1'), 
NULL, 
NULL, 
1, 
0, 
'' 
);

DB.DBA.XML_SET_NS_DECL ('mdr', 'http://mdr.semic.eu/def#', 2);
DB.DBA.XML_SET_NS_DECL ('vdm', 'http://vocabs.tenforce.com/def#', 2);
DB.DBA.XML_SET_NS_DECL ('adms','http://www.w3.org/ns/adms#', 2);

DB.DBA.VHOST_REMOVE (
	 lhost=>'*ini*',
	 vhost=>'*ini*',
	 lpath=>'/'
);

DB.DBA.VHOST_DEFINE (
	 lhost=>'*ini*',
	 vhost=>'*ini*',
	 lpath=>'/',
	 ppath=>'http://vocabs.tenforce.com/vdm/',
	 is_dav=>0,
	 def_page=>'',
	 ses_vars=>0,
	 opts=>vector ('browse_sheet', ''),
	 is_default_host=>0
);
